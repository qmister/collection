{
    "name": "tp5er/collections",
    "type": "library",
    "description": "A PHP library for representing and manipulating collections",
    "license": "MIT",
    "keywords": [
        "array",
        "collection",
        "hash",
        "str",
        "map",
        "set"
    ],
    "require": {
        "php": ">5.6",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-4": {
            "tp5er\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload-dev": {
        "psr-4": {
            "tp5er\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "fresh": [
            "rm -rf vendor composer.lock",
            "composer install"
        ],
        "tests": [
            "php vendor/bin/phpunit"
        ]
    }
}
